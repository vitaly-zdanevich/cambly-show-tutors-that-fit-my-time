{
  "manifest_version": 2,
  "name": "Cambly: tutors for my time",
  "description": "You have an hour of time and do not want to see the tutors who will be busy after 15 minutes - this extension will hide them.",
  "homepage_url": "https://github.com/vitaly-zdanevich/cambly-show-tutors-that-fit-my-time",
  "minimum_chrome_version": "60",
  "permissions": ["activeTab"],
  "version": "2018.08.03",

  "browser_action": {
    "default_icon": {
      "32": "ico.png"
    }
  },
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "applications": {
    "gecko": {
      "id": "{a5a46bdd-75dd-4478-a420-0682ae132270}",
      "strict_min_version": "57.0"
    }
  },
  "author": "Vitaly Zdanevich"
}
